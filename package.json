{
  "name": "capn-hook",
  "packageManager": "yarn@3.2.0",
  "type": "module",
  "main": "./build/index.js",
  "scripts": {
    "test": "yarn run check && yarn run lint",
    "check": "yarn run compile --noEmit",
    "lint": "xo",
    "compile": "tsc",
    "clean": "rimraf build",
    "prebuild": "yarn run clean",
    "build": "yarn run compile",
    "dev": "yarn run build --watch",
    "debug": "nodemon --inspect",
    "postinstall": "husky install",
    "schemagen": "yarn run build && node . schemagen"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa__router": "^8.0.11",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.25",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3",
    "xo": "^0.48.0"
  },
  "dependencies": {
    "@koa/router": "^10.1.1",
    "koa": "^2.13.4",
    "mkdirp": "^1.0.4",
    "source-map-support": "^0.5.21",
    "zod": "^3.14.4",
    "zod-to-json-schema": "^3.11.3"
  }
}
